{
  "SERVICES": [
    {
      "CONTROLLER": {
        "NAMESPACE": "https://oauth.vk.com/",
        "METHOD": [
          {
            "NAME": "authorize",
            "TYPE": "GET",
            "PARAMS": [
              { "key": "client_id", "value": "$CLIENT_ID" },
              { "key": "redirect_uri", "value": "$REDIRECT_URI" },
              { "key": "display", "value": "page" },
              { "key": "scope", "value": "friends" },
              { "key": "response_type", "value": "token" },
              { "key": "v", "value": "$VERSION" }
            ]
          }
        ]
      }
    },
    {
      "CONTROLLER": {
        "NAMESPACE": "https://api.vk.com/method/",
        "METHOD": [
          {
            "NAME": "account.getProfileInfo",
            "TYPE": "GET",
            "PARAMS": [
              { "key": "access_token", "value": "$ACCESS_TOKEN" },
              { "key": "v", "value": "$VERSION" }
            ]
          }
        ]
      }
    }
  ]
}